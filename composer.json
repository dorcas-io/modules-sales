{
    "name": "dorcas/modules-sales",
    "description": "Dorcas Hub Sales Module",
    "type": "library",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Ifeoluwa Olawoye",
            "email": "bolaji.olawoye@hostvillenigeria.org"
        }
    ],
    "require": {
        "php": "^7.4|8.1.7",
        "aws/aws-sdk-php": "^3.74",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "^8.0",
        "picqer/php-barcode-generator": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.10"
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Dorcas\\ModulesSales\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dorcas\\ModulesSales\\ModulesSalesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
